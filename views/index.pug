- var m = 1;
doctype html
html
  head
    title= title
  body(style="display: flex; justify-content: center;")
    div= timerStart
      div HTTP1 Images
      div Time load: 
        span(id="timeLoad")
      br
      while m <= 100
        if (m % 10 == 0)
          img(width="50" height="50" src=`${protocol}://${host}/download (${m++}).jpeg`)
          br
        else
          img(width="50" height="50" src=`${protocol}://${host}/download (${m++}).jpeg`)
  script
    include script.js